{
    "version": 2,
    "builds": [
      {
        "src": "wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/app-ads.txt",
        "dest": "/app-ads.txt"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/xjkSW/wsgi.py"
      }
    ]
  }
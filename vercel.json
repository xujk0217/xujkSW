{
    "version": 2,
    "builds": [
      {
        "src": "xjkSW/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/xjkSW/wsgi.py"
      }
    ],
      "rewrites": [
      { "source": "/app-ads.txt", "destination": "/app-ads.txt" }
    ]
  }
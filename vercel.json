{
    "version": 2,
    "builds": [
      {
        "src": "wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "api/app-ads.txt.py",
        "use": "@vercel/python"
      },
      {
        "src": "api/test.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/test",
        "dest": "/api/test.py"
      },
      {
        "src": "/app-ads.txt",
        "dest": "/api/app-ads.txt.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/wsgi.py"
      }
    ]
  }